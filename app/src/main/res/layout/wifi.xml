<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:padding="15dp"
                android:id="@+id/relLayout">

    <TextView
        android:id="@+id/ssid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:text="SSID"
        android:textSize="16sp"/>

    <TextView
        android:id="@+id/value1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:textSize="16sp"/>

    <Button
        android:id="@+id/button1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="10dp"
        android:text="Send Info"/>

    <TextView
        android:id="@+id/bssid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/ssid"
        android:layout_marginTop="19dp"
        android:text="TextView"/>

    <TextView
        android:id="@+id/value4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/bssid"
        android:layout_alignEnd="@+id/value1"
        android:layout_alignRight="@+id/value1"
        android:text="TextView"/>

    <TextView
        android:id="@+id/speed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/bssid"
        android:layout_marginTop="28dp"
        android:text="TextView"/>

    <TextView
        android:id="@+id/value6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_alignBaseline="@+id/speed"
        android:layout_alignBottom="@+id/speed"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"/>

    <TextView
        android:id="@+id/strength"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/speed"
        android:layout_alignRight="@+id/speed"
        android:layout_below="@+id/value6"
        android:layout_marginTop="24dp"
        android:text="TextView"/>

    <TextView
        android:id="@+id/value8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/strength"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:text="TextView"/>

    <TextView
        android:id="@+id/ipaddress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/strength"
        android:layout_marginTop="19dp"
        android:text="TextView"/>

    <TextView
        android:id="@+id/value10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/ipaddress"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:text="TextView"/>

    <TextView
        android:id="@+id/macaddress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="23dp"
        android:text="TextView"
        android:layout_below="@+id/ipaddress"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"/>

    <TextView
        android:id="@+id/value12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/macaddress"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:text="TextView"/>

    <TextView
        android:id="@+id/freq"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/macaddress"
        android:layout_marginTop="25dp"
        android:text="TextView"/>

    <TextView
        android:id="@+id/value14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/freq"
        android:layout_alignStart="@+id/value12"
        android:text="TextView"/>

</RelativeLayout>